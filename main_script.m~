%% Symbolic variables 
% joint angles 
syms q1 q2 q3 q4
Q = [q1 q2 q3 q4];

% link lengths 
syms l1 l2 l3 lE
L = [l1 l2 l3 lE]; 

%% Jacobean for end effector: 
J_E = jacobean(Q,L,end_effector_frame); 

%% Jacobean for COM of each link:
J_1 = jacobean(Q,L,1);
J_2 = jacobean(Q,L,2);
J_3 = jacobean(Q,L,3);
J_4 = jacobean(Q,L,4);

%% Symbolic Tau required to hold robot up given particular joint angle: 
Tau = torque_required(Q,L);

